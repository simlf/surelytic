<div class="container">
  <h1>hello <%= @orga.name %></h1>
  <h2>All your tests 🕶️</h2>
  <h3><%= link_to "create new project", new_project_path %></h3>

  <% @projects.each do |project| %>
    <h3><%= project.name %> | <%= link_to "👓️", project_path(project) %></h3>
    <% project.tests.each do |test| %>
      <p><%= test.name %> | <%= link_to "see details", project_test_path(project, test) %></p>
      <!-- info : /projects/:project_id/tests/:id -->
    <% end %>
  <% end %>
</div>
