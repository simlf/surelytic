      <h2><%=@test.name %></h2>

      <h3>Test details</h3>
      <div class="card background">
        <p> Test type : <%= @test.ga_report_type.capitalize %></p>
        <p> Regex used : "<%= @test.event_category_regex %>"</p>
      </div>

    <% if @last_result == nil %>
      <p> Sorry,<strong> No result yet</strong> üò¢ <br>Come back later</p>
    <% else %>
     </div>

  </div>

